<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="demo.third.com.exceldemo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />

    <application
        android:name=".app.CustomApplication"
        android:allowBackup="false"
        android:exported="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- 主页面 -->
        <activity
            android:name=".ui.activity.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <!-- 表格类 -->
        <activity
            android:name=".ui.activity.ExcelActivity"
            android:screenOrientation="portrait" />
        <!-- 欢迎页面 -->
        <activity
            android:name=".ui.activity.WelcomeActivity"
            android:screenOrientation="portrait"
            android:theme="@style/bestStart">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 网页h5 -->
        <activity
            android:name=".ui.activity.MyWebActivity"
            android:screenOrientation="portrait" />
        <!-- 登陆页面 -->
        <activity
            android:name=".ui.activity.LoginActivity"
            android:screenOrientation="portrait" />
        <!-- 注册页面 -->
        <activity
            android:name=".ui.activity.RegisterActivity"
            android:screenOrientation="portrait" />
        <!-- 友盟相关配置 -->
        <meta-data
            android:name="UMENG_APPKEY"
            android:value="${umeng_key}" />
        <meta-data
            android:name="UMENG_MESSAGE_SECRET"
            android:value="${umeng_secret}" />
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="${UMENG_CHANNEL}" />
        <meta-data
            android:name="android.webkit.WebView.EnableSafeBrowsing"
            android:value="true" />

        <!-- 强制横屏的h5 -->
        <activity
            android:name=".ui.activity.LandSpaceWebWebActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".ui.activity.TextToImageActivity"></activity>
    </application>

</manifest>