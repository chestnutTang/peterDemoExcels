<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/layout_height_46"
    android:background="@drawable/ic_include_back">

    <ImageView
        android:id="@+id/iv_backup"
        android:layout_width="@dimen/image_20"
        android:layout_height="@dimen/image_22"
        android:layout_gravity="start|center_vertical"
        android:layout_marginStart="@dimen/image_margin"
        android:contentDescription="@null"
        android:visibility="visible"
        app:srcCompat="@drawable/ic_arrow_left" />

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/layout_height_42"
        android:layout_gravity="center_horizontal"
        android:gravity="center"
        android:text="@string/title_input_info"
        android:textColor="@color/black"
        android:textSize="@dimen/text_size_20" />

    <TextView
        android:id="@+id/tv_jump"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end|center_vertical"
        android:layout_marginEnd="@dimen/image_margin"
        android:gravity="center"
        android:text="@string/txt_jump"
        android:textColor="@color/white"
        android:textSize="@dimen/text_size_18"
        android:visibility="visible" />

</FrameLayout>
